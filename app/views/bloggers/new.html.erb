<h2> Create a new Blogger </h2>
<%= form_for @blogger do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br><br>
<%= f.label :age %>
<%= f.number_field :age %>
<br><br>
<%= f.label :bio %>
<%= f.text_area :bio %>
<br><br>
<%= f.submit %>
<% end %>
<br>
<%= link_to "All Bloggers", bloggers_path %>
